msgid "API Key Scope"
msgstr "Âmbito da chave API"

#: uw_api_keys/admin.py:40
msgid "API Key Scopes - Leave empty for granting permission to everything"
msgstr "Âmbitos da chave API - Deixar em branco para conceder permissão a tudo"

#: uw_api_keys/admin.py:64
msgid "Has expired"
msgstr "Já expirou"

#: uw_api_keys/admin.py:67
msgid "Yes"
msgstr "Sim"

#: uw_api_keys/admin.py:68
msgid "No"
msgstr "Não"

#: uw_api_keys/admin.py:77
#, python-brace-format
msgid ""
"Your API key: '{key}'. Please store it somewhere safe - you will not be able "
"to see it again."
msgstr ""
"A sua chave API: '{key}'. Guarde-a num local seguro - não poderá voltar a vê-"
"la."

#: uw_api_keys/conf.py:18
msgid "Provided API key is invalid"
msgstr "A chave API fornecida é inválida"

#: uw_api_keys/conf.py:20
msgid "Provided API key is valid, but has insufficient scopes"
msgstr "A chave API fornecida é válida, mas tem âmbitos insuficientes"

#: uw_api_keys/conf.py:23
#, python-format
msgid "Provided API key is valid, but origin '%s' is not."
msgstr "A chave API fornecida é válida, mas a origem \"%s\" não é."

